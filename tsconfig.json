{
  "compilerOptions": {
    "target": "ES2020",
    "experimentalDecorators": true, //tsringe
    "emitDecoratorMetadata": true, //tsringe
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
