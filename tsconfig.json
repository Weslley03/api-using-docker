{
  "compilerOptions": {
    "target": "ES2020",
    "experimentalDecorators": true, //tsringe
    "emitDecoratorMetadata": true, //tsringe
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["src/**/*", "jest.setup.ts"],
  "exclude": ["node_modules"]
}
